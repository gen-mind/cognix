blobstatusid = case when @blobstatusid < blobstatusid then
blobstatusid else @blobstatusid end,

lastupdate = @lastupdate


-----

WHERE parentid = @tileid

IF(@blobstatusid = dbo.fn_GetBlobStatus('DELETED'))

BEGIN

DELETE

FROM t_tile_voting

WHERE tileid = @tileid

END

SET NOCOUNT OFF

RETURN 0

